<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core" 
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
    <Dialog id="idConfirmarDeletarEquipamento" afterClose="onDialogClose" >
    <customHeader>
        <Bar>
            <contentLeft>
                <Title 
					text="{equipamentoSelecionadoLista>/nome}" 
					level="H1"/>
            </contentLeft>
        </Bar>
    </customHeader>
        <content>
       	    
        </content>
        <buttons>
            <Button text="{i18n>btnFechar}" press="aoPressionarFecharDetalhes"/>
            <Button text="{i18n>btnConfirmar}" press="aoConfirmarDeletarEquipamento"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>