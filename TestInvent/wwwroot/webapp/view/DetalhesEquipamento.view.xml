 <mvc:View
    controllerName="ui5.testinvent.controller.DetalhesEquipamento"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns:f="sap.f"
	>
	 <f:DynamicPage id="dynamicPageId" headerExpanded="true">
        <f:title>
            <f:DynamicPageTitle>
                <f:heading navButtonPress=".onNavBack">
                    <Title text="{equipamento>/nome}"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <VBox>
					<ObjectIdentifier
						title="{equipamento>/tipo}"
						/>
                    <ObjectStatus
						text="{equipamento>/quantidadeEmEstoque}"
						state="Information"
						title="{i18n>tituloquantidade}"/>
					<ObjectAttribute
						title="{i18n>dataInclusao}"
						text="{
							path: 'equipamento>/dataDeInclusao',
							type: 'sap.ui.model.type.Date',
							formatOptions: {
								style: 'short'
							}
						}"/>
					<ObjectAttribute
						title="{i18n>descricaoEquipamento}"
						text="{equipamento>/descricaoEquipamento}"/>
					<ObjectAttribute title="{i18n>disponibilidadeEquipamento}" text="{= ${equipamento>/temEmEstoque} === true ? 'Sim' : 'NÃ£o' }" />
                </VBox>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <Text text=""/>
        </f:content>
    </f:DynamicPage>
</mvc:View> 