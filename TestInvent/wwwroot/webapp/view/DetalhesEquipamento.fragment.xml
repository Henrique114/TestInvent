<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
>
    <Dialog 
        id=""
		type="Message" 
        afterOpen=""
		title="{i18n>}"
		contentHeight="100%"
		contentWidth="700px"
        horizontalScrolling="false">
        <content>
            <FlexBox
                justifyContent="Center"
                class="sapUiTinyMarginEnd">
                <HBox>
                    <c:Fragment fragmentName="" type="XML" />
                </HBox>
            </FlexBox>
            <c:Fragment fragmentName="" type="XML" />
        </content>
        <endButton>
            <Button text="{i18n>}" width="90px" type="Emphasized" press="aoClicarEmSair"/>
        </endButton>
    </Dialog>
</c:FragmentDefinition>