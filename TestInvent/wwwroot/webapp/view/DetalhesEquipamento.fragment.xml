<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <Dialog id="idDialog" title="{modeloDialogo>/nome}" afterClose="onDialogClose" 
      >
    <ObjectHeader
      responsive="true"
      number="{modeloDialogo>/quantidadeEmEstoque}"
      numberUnit="Unidade(s)"
      title="{modeloDialogo>/tipo}"
      >
      <attributes>
      <ObjectAttribute title="{i18n>disponibilidadeEquipamento}" text="{modeloDialogo>/temEmEstoque}" />
        <ObjectAttribute
          title="{i18n>dataInclusao}"
            text="{
                path: 'modeloDialogo>/dataDeInclusao',
                type: 'sap.ui.model.type.Date',
                formatOptions: {
                    style: 'short'
            }
          }"/>
          <Label text="These controls are within one multi-root Fragment:" />
          <Input />
          
      </attributes>
      
       <!-- <content>
                <Text title="{i18n>disponibilidadeEquipamento}" text="{modeloDialogo>/temEmEstoque}" />
                <Text title="{i18n>dataInclusao}" text="{
                path: 'modeloDialogo>/dataDeInclusao',
                type: 'sap.ui.model.type.Date',
                formatOptions: {
                    style: 'short'
                    }
                 }"/>
        </content> -->
    </ObjectHeader>
      <Button text="Fechar" press="aoPrecionarFechar"/>
    </buttons>
  </Dialog>
</core:FragmentDefinition>